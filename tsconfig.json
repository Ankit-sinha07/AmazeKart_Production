{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/tshirts.js", "pages/signup.js", "pages/orders.js1", "pages/mugs.js", "pages/login.js", "pages/hoodies.js", "pages/contact.js", "pages/checkout.js", "pages/about.js"  ],
  "exclude": [
    "node_modules"
  ]
}
